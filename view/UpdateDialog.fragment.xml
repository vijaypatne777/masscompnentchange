<core:FragmentDefinition xmlns="sap.m" xmlns:l="sap.ui.layout" xmlns:core="sap.ui.core" xmlns:f="sap.ui.layout.form">
	<Dialog id="updateDialog" class="sapUiContentPadding"  title="{i18n>titleUpdate}" draggable="true">
		<content>
			<Text text="{i18n>updateCompDialogTxt}"></Text>
			</content>
		<beginButton>
			
			<Button id="okButtonUpdate" press="onOkUpdateComp" text="{i18n>updateCompYesText}"/>
		</beginButton>
		<endButton>
			<Button id="closeButtonUpdate" press="onCancelUpdateComp" text="{i18n>updateCompNoText}"/>
		</endButton>
	</Dialog>
</core:FragmentDefinition>